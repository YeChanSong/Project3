<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <script src="/javascripts/jquery-3.5.1.min.js"></script>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
    <script>
        $(function(){
            $("#btnRtn").click(
                function(){
                    location.href="/";
                })
        });
    </script>
</head>
<body>
    <h1><%= title %></h1>

    <form action="/account/info" method="get">
        <table border="1">
            <tr>
                <td width="50">구분</td>
                <td width="100">ID</td>
                <td width="100">이름</td>
                <td width="200">PHONE</td>
            </tr>
            <%
                for(var i=0; i<rows.length; i++){
                    var item = rows[i];
            %>
            <tr>
                <td><%=item.AUTH%></td>
                <td><a href="/account/info/<%=item.ID%>"><%=item.ID%></a></td>
                <td><%=item.NAME%></td>
                <td><%=item.PHONE%></td>
            </tr>
            <%
                }
            %>
            <tr>
                <td colspan="2">
                    <input type="button" id="btnRtn" name="return" value="돌아가기">
                </td>
            </tr>
        </table>
    </form>
</body>
</html>

